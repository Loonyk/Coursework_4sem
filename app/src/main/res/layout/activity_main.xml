<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#22C4E0"
    tools:context=".MainActivity" >

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/textView"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="210dp"
            android:height="80sp"
            android:fontFamily="serif"
            android:gravity="top|center_horizontal"
            android:text="МОЙ СКЛАД"
            android:textColor="#292E75"
            android:textSize="44sp"
            android:textStyle="normal" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="500dp"
            android:height="25sp"
            android:fontFamily="sans-serif-light"
            android:gravity="top|center_horizontal"
            android:text="Курсовая работа"
            android:textColor="#292E75"
            android:textSize="15sp"
            android:textStyle="normal" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="520dp"
            android:height="25sp"
            android:fontFamily="sans-serif-light"
            android:gravity="top|center_horizontal"
            android:text="по Разработке программных приложений"
            android:textColor="#292E75"
            android:textSize="15sp"
            android:textStyle="normal" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="540dp"
            android:height="25sp"
            android:fontFamily="sans-serif-light"
            android:gravity="top|center_horizontal"
            android:text="студента группы ИКБО-01-17"
            android:textColor="#292E75"
            android:textSize="15sp"
            android:textStyle="normal" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="560dp"
            android:height="25sp"
            android:fontFamily="sans-serif-light"
            android:gravity="top|center_horizontal"
            android:text="Тюриной Валерии"
            android:textColor="#292E75"
            android:textSize="15sp"
            android:textStyle="normal" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                app:srcCompat="@drawable/box" />
        </LinearLayout>

    </FrameLayout>

</android.support.constraint.ConstraintLayout>